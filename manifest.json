{
    "name": "Chrome Tab Manager",
    "version": "1.0",
    "manifest_version": 3,
    "description": "manage chrome tabs more conveniently",
    "default_locale": "en",
    "permissions": ["storage", "tabs"],
    "action": {
        "default_title": "Tab Manager",
        "default_popup": "index.html"
    }
}